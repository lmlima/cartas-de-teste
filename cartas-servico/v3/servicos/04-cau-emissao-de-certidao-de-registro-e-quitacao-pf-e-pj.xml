                        <descricao><![CDATA[servicos.caubr.gov.br]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id=""/>
    <segmentos-da-sociedade>
